@page
@model GymGenius.Ui.Pages.SignInModel

<h2>Sign In</h2>

<form method="post">
    <label>Email:</label>
    <input asp-for="LoginUser.Email" /><br />

    <label>Password:</label>
    <input asp-for="LoginUser.Password" type="password" /><br />

    <button type="submit">Sign In</button>
</form>

<p>@Model.Message</p>
